{
  "name": "js-tdd-course",
  "version": "1.0.0",
  "description": "Nossa biblioteca do curso de JS com TDD na pr√°tica.",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "start": "./node_modules/.bin/webpack-dev-server --open"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gianluska/Spotify-Wrapper-Player.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Gianlucca Claudino <gianluccaclaudino@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gianluska/Spotify-Wrapper-Player/issues"
  },
  "homepage": "https://github.com/Gianluska/Spotify-Wrapper-Player#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "husky": "^0.11.9",
    "jsdom": "^16.2.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "spotify-wrapper": "^2.1.0"
  }
}
